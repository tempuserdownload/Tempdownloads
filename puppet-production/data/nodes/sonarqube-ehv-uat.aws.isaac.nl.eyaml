---
syslocation: VM in AWS 
sysdescription: Sonarqube uat Playground
sonarqube::site: sonarqube-uat-eindhoven.isaac.nl
sonarqube::site_alias: [ 'sonarqube-uat.eindhoven.io-external.com']
depsonarqubetrack::ecs_cluster: sonarqubeuat
isaac::docker::registry_mirror: false
isaac::docker::daemon_json_custom:
  data-root: "/data/docker/"
  log-driver: "json-file"
  log-opts:
    max-size: "2g"
    max-file: "4"
    compress: "true"
include_classes:
  - sonarqube::configure_sonarqube